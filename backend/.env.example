FLASK_CONFIG=development
SECRET_KEY=dev-secret-key-change-in-production
JWT_SECRET_KEY=jwt-secret-change-in-production

# Database (Docker Postgres service)
DATABASE_URL=postgresql://postgres:postgres@postgres_docker:5432/visamadeeasy_docker
DEV_DATABASE_URL=postgresql://postgres:postgres@localhost:5433/visamadeeasy_docker
TEST_DATABASE_URL=sqlite:///:memory:

# CORS
CORS_ORIGINS=["http://localhost:5173","http://frontend:5173"]

# Redis / Celery (Docker network)
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# AI
GEMINI_API_KEY=YOUR-API-KEY
AI_MODEL=gemini-1.5-flash

# Uploads
MAX_CONTENT_LENGTH=16777216

